import { BaseGenerator } from '../base-generator.mjs';

class ValueGenerator extends BaseGenerator {
    /**
     * Converts a value node to a string.
     *
     * @param node Value node.
     * @returns Raw string.
     */
    static generate(node) {
        return node.value;
    }
}

export { ValueGenerator };
