'use strict';

var baseGenerator = require('../base-generator.js');
var listItemsGenerator = require('./list-items-generator.js');

class DomainListGenerator extends baseGenerator.BaseGenerator {
    /**
     * Converts a domain list node to a string.
     *
     * @param node Domain list node.
     *
     * @returns Raw string.
     */
    static generate(node) {
        return listItemsGenerator.ListItemsGenerator.generate(node.children, node.separator);
    }
}

exports.DomainListGenerator = DomainListGenerator;
