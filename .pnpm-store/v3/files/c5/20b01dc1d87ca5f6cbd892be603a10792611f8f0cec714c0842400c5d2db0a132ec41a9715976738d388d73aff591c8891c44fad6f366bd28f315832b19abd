import { BaseDeserializer } from './base-deserializer';
import { type EmptyRule } from '../nodes';
import { type InputByteBuffer } from '../utils/input-byte-buffer';
export declare class EmptyRuleDeserializer extends BaseDeserializer {
    /**
     * Deserializes an empty rule node from binary format.
     *
     * @param buffer ByteBuffer for reading binary data.
     * @param node Destination node.
     */
    static deserialize(buffer: InputByteBuffer, node: EmptyRule): void;
}
